    "start": "dotenv -e .env --  node ./dist/index.js",
    "start:debug": "dotenv -e .env -- node --inspect=0.0.0.0:9229 ./dist/index.js",
    "dev": "dotenv -e .env -- docker-compose up --build -d",
    "dev:stop": "dotenv -e .env -- docker-compose down",
    "watch": "dotenv -e .env.development -- nodemon src/index.ts",
    "codegen": "graphql-codegen --config codegen.yaml",
    "clean": "shx rm -rf ./dist",
